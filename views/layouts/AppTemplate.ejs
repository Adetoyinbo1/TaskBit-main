                    <div class="container">
                        <div class="row" is="dmx-repeat" id="repeat_notifications" dmx-bind:repeat="Notification.data.query">
                            <div class="d-flex align-items-center col-12 mb-2">
                                <div class="d-flex align-items-center">
                                    <img width="40" height="40" class="rounded-circle" dmx-bind:src="creatorImg" dmx-show="Conversationi_id">
                                    <div class="d-flex notif-icon-circle justify-content-center align-items-center notif-icon-circle-green" dmx-show="Task_id">
