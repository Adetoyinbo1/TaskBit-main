                        <div class="mb-3">
                            <label for="NewEmployee_LastName" class="form-label">Last name</label>
                            <input type="text" class="form-control rounded" id="NewEmployee_LastName" name="LastName" aria-describedby="input1_help">
                        </div>
                        <div class="mb-3">
                            <label for="NewEmployee_LastName1" class="form-label">Email</label>
                            <input type="text" class="form-control rounded" id="NewEmployee_LastName1" name="Email" aria-describedby="input1_help">
                        </div>
                        <div class="mb-3">
                            <label for="NewEmployee_LastName2" class="form-label">Role</label>
                            <input type="text" class="form-control rounded" id="NewEmployee_LastName2" name="Role" aria-describedby="input1_help">
                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-dark" data-bs-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" dmx-on:click="Add_Employee_form.submit()">Add employee</button>
                </div>
            </div>
        </div>
    </div>


    <div class="modal" id="Add_task" is="dmx-bs5-modal" tabindex="-1">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add new task</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <form id="Add_task_form" class="border Add_task_form" is="dmx-serverconnect-form" method="post" action="/api/Tasks/add_task" dmx-on:success="Add_task_form.reset();Add_task.hide();New_task_list.add(Add_task.Add_task_form.data.task_id)">
                        <input id="CompanyId" name="CompanyId" class="form-control" dmx-bind:value="content.company_select.value" type="hidden">
                        <input id="ReferenceTaskId" name="ReferenceTaskId" type="hidden" class="form-control" dmx-bind:value="task_id.value">
                        <div class="mb-3">
                            <label for="TaskTitle" class="form-label">Task title</label>
                            <input type="text" class="form-control rounded" id="TaskTitle" name="TaskTitle" aria-describedby="input1_help">
                        </div>
                        <div class="mb-3">
                            <label for="Task_description" class="form-label">Task description</label>
                            <input type="text" class="form-control rounded" id="Task_description" name="Task_description" aria-describedby="input1_help">
                        </div>

                        <div class="mb-3">
                            <label for="select_task_type" class="form-label">Task type</label>
                            <select id="select_task_type" class="form-control rounded" name="Type">
                                <option value="Personal">Personal</option>
                                <option value="Team">Team</option>
                            </select>
                        </div>
                        <div class="mb-3">
                            <label for="TASK_DueDate" class="form-label">Due date</label>
                            <input class="form-control rounded" id="TASK_DueDate" name="Final_date" aria-describedby="input1_help" type="text" is="dmx-date-picker" dropsup="true" timepicker="" use24hours="true">
                        </div>
                        <div class="d-flex mb-3 align-items-center">
                            <div class="mb-0">
                                <label for="task_icon_inp" class="form-label">Task icon</label>
                                <input type="file" class="form-control" id="task_icon_inp" name="Avatar" aria-describedby="input3_help">
                            </div>
                            <img class="rounded-circle ms-3" width="40" height="40" dmx-bind:src="task_icon_inp.file.dataUrl">
                        </div>


                    </form>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-dark" data-bs-dismiss="modal">Close</button>
                    <button type="button" class="btn btn-primary" dmx-on:click="Add_task_form.submit()">Add task</button>
                </div>
            </div>
        </div>
    </div>

    <dmx-smooth-scroll id="scroll1"></dmx-smooth-scroll>
    <div is="dmx-browser" id="browser"></div>


    <div class="offcanvas off-canvas offcanvas-start" id="offcanvas_Navigation" is="dmx-bs5-offcanvas" tabindex="-1">
        <div class="offcanvas-header">
            <h5 class="offcanvas-title">Navigation</h5>
            <button type="button" class="btn-close text-reset" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">
            <div class="d-flex dropDown-menu flex-column">
                <p class="text-center rounded pt-2 pb-2">Dashboard</p>
                <p class="text-center rounded mt-2 pt-2 pb-2">Tasks</p>
                <p class="text-center rounded mt-2 pt-2 pb-2">Chats</p>
                <p class="text-center rounded mt-2 pt-2 pb-2">Settings</p>
                <p class="text-center rounded mt-5 pt-2 pb-2 bg-warning" dmx-on:click="log_out.load();browser.goto('/signup_page')">Sign out</p>
            </div>
        </div>
    </div>
    <dmx-value id="Chat_switch"></dmx-value>
    <dmx-value id="personal_company_tasks" dmx-bind:value="'my tasks'"></dmx-value>
    <dmx-value id="task_id"></dmx-value>
    <dmx-value id="msg_id"></dmx-value>
    <dmx-value id="Edit_task_id"></dmx-value>
    <dmx-value id="User_id"></dmx-value>
    <dmx-value id="hover_var"></dmx-value>
    <dmx-value id="mobile_sideBar"></dmx-value>
    <dmx-value id="leftSideBar_toggle" dmx-bind:value="0"></dmx-value>
    <dmx-serverconnect id="TaskView" url="/api/Tasks/Added_task_edit_query" dmx-param:id="task_id.value" sockets="true"></dmx-serverconnect>
    <dmx-serverconnect id="new_task_list" url="/api/Tasks/new_task_list" sockets="true" dmx-param:task_ids="New_task_list.items"></dmx-serverconnect>
    <dmx-serverconnect id="Single_user" url="/api/App_query/Single_user" dmx-param:userid="User_id.value"></dmx-serverconnect>
    <dmx-serverconnect id="log_out" url="/api/log_out" noload></dmx-serverconnect>


    <dmx-serverconnect id="Task_conversation_chat_list" url="/api/Conversation/Chat_list_display/Task_conversation_list" sockets="true"></dmx-serverconnect>
    <dmx-serverconnect id="All_users" url="/api/App_query/All_Users" dmx-param:companyid="content.company_select.value" sockets="true"></dmx-serverconnect>
    <dmx-serverconnect id="Users_4_dm" url="/api/Conversation/Chat_list_display/Users_4_dm"></dmx-serverconnect>

    <dmx-serverconnect id="RemindersQuery" url="/api/Reminder/QueryReminder" sockets="true"></dmx-serverconnect>

    <dmx-serverconnect id="current_chat_pins" url="/api/Conversation/User_conversation_pins/Query_chat_pins" dmx-param:companyid="1" dmx-param:task_status="0" dmx-param:convo_id="Conversation_id.value" sockets="true"></dmx-serverconnect>

    <dmx-serverconnect id="All_company_tasks" url="/api/Tasks/All_company_tasks" dmx-param:companyid="content.company_select.value" dmx-param:task_status=""></dmx-serverconnect>
    <dmx-serverconnect id="Notification" url="/api/Notification/Query_notifications" dmx-param:companyid="content.company_select.value" dmx-param:task_status="" sockets="true"></dmx-serverconnect>
    <script src="/bootstrap/5/js/bootstrap.bundle.min.js"></script>
</body>

</html>