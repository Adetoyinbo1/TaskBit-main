      url: "/api/Conversation/Chat_list_display/Direct_chats",
      site: "Taskbit",
      name: "sc",
      output: true
    }
  },
  {
    repeat: {
      repeat: "{{sc.data.repeat_currentUser}}",
      outputFields: [],
      exec: {
        steps: {
          repeat: {
            repeat: "{{repeat_query}}",
            outputFields: ["id"],
            exec: {
              steps: {
                run: {action: "{{Dm_userId.add(UserId)}}", output: true}
              }
            },
            name: "repeat_user_list",
            output: true
          }
        }
      },
      name: "repeat_chat_list",
      output: true
    }
  }
]</script>
